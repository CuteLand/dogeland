<!-- MainPage -->
<items>
    <group>
        <action>
            <title>Enable/Disable</title>
            <desc>Manage your GNU/Linux containers </desc>
             <param name="start">
                <option value="start_auto">[auto]</option>
                <option value="start_chroot">[chroot]</option>
                <option value="start_proot">[proot]</option>
            </param>
            <param name="addcmd" label="AddOptions" title="" placeholder="" type="text" />
            <set>$TOOLKIT/cli.sh $start</set>
            </action>
            </group>
            
            <group>
              <action>
            <title>RunShellCommand</title>
            <desc>Quickly run container shell commands</desc>
            <param name="cmd2" label="" title="" placeholder="" type="text" />
             <param name="exec">
                <option value="exec_auto">[auto]</option>
                <option value="exec_chroot">[chroot]</option>
                <option value="exec_proot">[proot]</option>
                <option value="exec_localshell">[local-Shell]</option>
            </param>
            <set>$TOOLKIT/cli.sh $exec</set>
        </action>
    </group>
</items>


