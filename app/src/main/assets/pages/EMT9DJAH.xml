<?xml version="1.0" encoding="UTF-8" ?> 
<!-- InstallPage -->
<group>
      <action>
            <title>安装→tgz</title>
            <param name="file" label="包文件" type="file"
                suffix="tgz"
                value="" />
            <param name="rootfs2" label="安装路径" title="" placeholder="" type="text" />
             <param name="file2" label="包文件" title="注意:↓无法得到存储权限时使用" placeholder="" type="text" />
                <param name="USER_NAME" label="登录用户" title="" placeholder="" type="text" />
                <param name="USER_PASSWORD" label="登录密码" title="" placeholder="" type="text" />
                <param name="datas" label="安装到私有数据目录" type="bool" />
                <param name="type">
                <option value="/bin/sh /dogeland/cli.sh dropbear_start">dropbear</option>
                <option value="/bin/sh /dogeland/cli.sh sshd_start">sshd</option>
                <option value="/bin/sh /dogeland/cli.sh vncserver_start">vncserver</option>
            </param>
            <set>
            sh $TOOLKIT/cli.sh deploy_linux
            </set>
        </action>
       
        </group>
        
