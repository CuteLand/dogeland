<?xml version="1.0" encoding="UTF-8" ?>
<!-- StorePage -->
 <group title="Rootfs ">
         <action>
            <title>Alpine</title>
            <desc>apk系,8+mb,需要正常连接到Github</desc>
            <param name="USER_NAME" label="登录用户" title="" placeholder="" type="text" />
            <param name="USER_PASSWORD" label="登录密码" title="" placeholder="" type="text" />
            <set>
            curl -O https://raw.githubusercontent.com/WhiteSky-Team/dogeland-assets/master/rootfs/alpine/$platform.tgz
            export file="$(pwd)/$platform.tgz"
            export datas=1
            export rootfs2="alpine-fs/"
            sh $TOOLKIT/cli.sh deploy_linux
            rm -rf $platform.tgz
            </set>
        </action>
         <text>
        <slices>
            <slice run="am start -n android/com.android.internal.app.PlatLogoActivity" size="15" color="#00000000" u="1">  </slice>
        </slices>
    </text>
        </group>
<group title="补丁 ">
<action>
            <title>Ashmem</title>
            <desc>只在运行某些图形界面程序出现问题时，才使用此补丁</desc>
            <param name="archs">
                <option value="arm_64">arm64</option>
                <option value="arm">arm</option>
                <option value="x86">x86</option>
                <option value="x86_64">x86_64</option>
            </param>
            <set>sh $TOOLKIT/cli.sh shmem_patch</set>
</action>
</group>

<group>
<action>
            <title>XstartupAdd</title>
            <desc>只在启动图形界面只有X时，才使用此补丁</desc>
            <param name="archs">
                <option value="startxfce4">xfce</option>
                <option value="startlxde">lxde</option>
                <option value="export PULSE_SERVER=tcp:0.0.0.0:4713">PulseAudio</option>
            </param>
            <set>sh $TOOLKIT/cli.sh xstartup_add</set>
</action>
</group>

<group>
<action>
            <title>DebianGroupAdd</title>
            <desc>只在登录终端提示缺少group组时，才使用此补丁</desc>
            <param name="bug" label="" title="请输入缺少的group组ID" placeholder="" type="text" />
            <set>sh $TOOLKIT/cli.sh debiangroup_add</set>
</action>
</group>

<group>
<action>
            <title>FuckQ-dash</title>
            <desc>使sh从垃圾的dash更换为bash</desc>
            <param name="type">
                <option value="sh_patcher">安装</option>
                <option value="">取消</option>
            </param>
            <set>sh $TOOLKIT/cli.sh $type</set>
</action>
</group>

<group>
<action>
            <title>fs-perm</title>
            <desc>全局更改Linux文件系统权限,用于解决一些bug</desc>
           <param name="perm" label="" title="" placeholder="0755" type="text" />
            <set>chmod -R $perm $rootfs/</set>
</action>
</group>

<group>
<action>
            <title>fix-sudo</title>
            <desc>修复sudo失效问题</desc>
            <set>sh $TOOLKIT/cli.sh fix_sudo</set>
</action>
</group>

<group>
<action>
            <title>PRootSECCOMP</title>
            <desc>开启PRootSECCOMP内核补丁</desc>
            <param name="type">
                <option value="enable_proot_seccomp">安装</option>
                <option value="">取消</option>
            </param>
            <set>sh $TOOLKIT/cli.sh $type</set>
</action>
</group>

<group title="插件 ">
<action>
            <title>安装插件</title>
            <param name="file" label="插件包" type="file"
                suffix="zip"
                value="" editable="true" />
            <set>
            sh $TOOLKIT/cli.sh plugin_installer
            </set>
        </action>
</group>

 <group>
   <page config="pages/hp4zq7bl.xml" title="GNU/Linux 扩展组件" />
 </group>
 
<group>
         <action>
            <title>QEMU-User-Emulator</title>
            <desc>开启PRoot/Chroot跨平台运行工具</desc>
            <set>
            echo "- 正在尝试打开浏览器🐳"
            am start --user 0 -a android.intent.action.VIEW -d https://github.com/WhiteSky-Team/dogeland-assets > /dev/null
            echo "- 完成🐳"
            </set>
        </action>
        </group>
        <group>
